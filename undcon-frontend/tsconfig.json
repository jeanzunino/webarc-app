{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@app/*": ["src/app/*"],
      "@environment/*": ["src/environments/*"],
      "@core/*": ["src/app/core/*"],
      "@service/*": ["src/app/core/service/*"],
      "@component-generic-list/*": ["src/app/core/component/generic-list/*"],
      "@guard/*": ["src/app/core/guard/*"],
      "@model/*": ["src/app/core/model/*"],
      "@enum/*": ["src/app/core/enum/*"]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
